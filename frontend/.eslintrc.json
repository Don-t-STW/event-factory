{
  "root": true,
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    "naver"
  ],
  "rules": {
    "class-methods-use-this": "off",
    "no-console": "off",
    "quotes": [
      "error",
      "single"
    ],
    "object-curly-spacing": [
      "error",
      "always"
    ],
    "comma-dangle": [
      "error",
      "always"
    ]
  }
}